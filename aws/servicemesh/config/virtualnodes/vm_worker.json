{
  "clientToken": "votemesh_token",
  "meshName": "votemesh",
  "spec": {
    "backends": [
      "database.votemesh.local",
      "queue.votemesh.local"
    ],
    "listeners": [
      {
        "portMapping": {
          "name": "ingress",
          "port": 8040,
          "protocol": "http"
      }
    }
    ],
    "serviceDiscovery": {
      "dns": {
        "serviceName": "worker.votemesh.local"
      }
    }
  },
  "virtualNodeName": "worker_node"
}
