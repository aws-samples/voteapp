{
  "clientToken": "votemesh_token",
  "meshName": "votemesh",
  "routeName": "reports",
  "spec": {
    "httpRoute": {
      "action": {
        "weightedTargets": [
          {
            "virtualNode": "reports_node",
            "weight": 1
          }
        ]
      },
      "match": {
        "prefix": "*"
      }
    }
  },
  "virtualRouterName": "reports_router"
}
